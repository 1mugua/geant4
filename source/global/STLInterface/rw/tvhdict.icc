template< class K >
unsigned G4Hash<K>::operator()( const K& key ) const {
    return G4Hash<K>::fhashfun( key );
}

template < class K > 		
void G4Hash<K>::SetHashFun( unsigned (*hashfun)( const K& ) ) {
  fhashfun = hashfun;
}


